{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:performance": "cd src/packages/performance && webpack --config webpack.config.js --mode development --watch",
    "build:performance": "cd src/packages/performance && webpack --config webpack.config.js --color",
    "build:next": "next build --turbopack",
    "build": "npm run build:performance && echo '\\033[32mPerformance build completed\\033[0m' && npm run build:next",
    "dev": "npm run dev:performance & next dev --turbopack",
    "start": "next start"
  },
  "dependencies": {
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sass": "^1.92.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^4",
    "typescript": "^5",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "ts-loader": "^9.5.4"
  }
}
